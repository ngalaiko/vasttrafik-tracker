{
  "name": "vasttrafik-tracker",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^24.4.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "pnpm run -r dev",
    "format": "pnpm run -r --include-workspace-root /^format:/",
    "clean": "pnpm run -r --include-workspace-root /^clean:/",
    "build": "pnpm run -r build",
    "download-lines": "node --experimental-strip-types ./scripts/download-lines.ts",
    "format:js": "eslint --fix .",
    "format:prettier": "prettier --experimental-cli --write ."
  },
  "devDependencies": {
    "@types/node": "24.0.15",
    "eslint": "9.31.0",
    "eslint-plugin-svelte": "3.11.0",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.36.8",
    "svelte-check": "4.3.0",
    "svelte-eslint-parser": "1.3.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.38.0"
  }
}
